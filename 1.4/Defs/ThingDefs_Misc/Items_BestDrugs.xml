<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="MakeableDrugBase">
		<defName>RI_Drug_MedullaOblongataElixir</defName>
		<label>【一品】理髓丹</label>
		<description>一品丹药，飞速治疗各种伤口，随机修复一个伤疤。</description>
		<graphicData>
			<texPath>Things/Item/Drugs/Elixir/RI_Drug_MedullaOblongataElixir</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<rotatable>false</rotatable>
		<statBases>
			<Nutrition>0.05</Nutrition>
			<WorkToMake>1500</WorkToMake>
			<MarketValue>553</MarketValue>
			<Mass>0.1</Mass>
		</statBases>
		<stackLimit>1</stackLimit>
		<techLevel>Industrial</techLevel>
		<ingestible>
			<foodType>Processed, Fluid</foodType>
			<baseIngestTicks>80</baseIngestTicks>
			<ingestSound>Ingest_Inject</ingestSound>
			<preferability>NeverForNutrition</preferability>
			<ingestCommandString>服用 {0}</ingestCommandString>
			<ingestReportString>服用 {0} 中</ingestReportString>
			<useEatingSpeedStat>false</useEatingSpeedStat>
			<chairSearchRadius>0</chairSearchRadius>
			<outcomeDoers>
				<li Class="IngestionOutcomeDoer_GiveHediff">
					<hediffDef>RI_RepairAwayHediff</hediffDef>
					<severity>1.00</severity>
                    <doToGeneratedPawnIfAddicted>true</doToGeneratedPawnIfAddicted>
				</li>
			</outcomeDoers>
		</ingestible>
		<recipeMaker>
			<researchPrerequisite>GoJuiceProduction</researchPrerequisite>
			<soundWorking>Recipe_Drug</soundWorking>
		</recipeMaker>
		<thingCategories>
			<li>RI_Category_Drugs</li>
		</thingCategories>
		<drawGUIOverlay>false</drawGUIOverlay>
	</ThingDef>

	<RecipeDef ParentName="RI_RecipeBase_withDanStove">
		<defName>RI_MedullaOblongataElixir</defName>
		<label>炼制理髓丹</label>
		<description>炼制理髓丹</description>
		<jobString>炼制丹药</jobString>
		<workAmount>2500</workAmount>
		<recipeUsers>
			<li>RI_Building_Cauldrons</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>RI_Resource_LiquidJade</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>RI_Resource_PiloseAntler</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>RI_Resource_LanternBerries</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>RI_Resource_ElephantTuskPowder</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<products>
			<RI_Drug_MedullaOblongataElixir>1</RI_Drug_MedullaOblongataElixir>
		</products>
		<fixedIngredientFilter>
			<thingDefs>
				<li>WoodLog</li>
			</thingDefs>
		</fixedIngredientFilter>
	</RecipeDef>

